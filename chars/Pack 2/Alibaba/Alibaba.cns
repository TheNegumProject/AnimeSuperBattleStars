[State 0, BindToRoot] ; CROUCH
type = BindToRoot
trigger1 = root,stateno = [10,12]
pos = -3*Helper(999999),Fvar(16),-30*Helper(999999),Fvar(16)

[State 0, BindToRoot] ; WALK
type = BindToRoot
trigger1 = (root,stateno = 20 && root,vel x < 0) || (root,stateno = 2070)
pos = 3*Helper(999999),Fvar(16),-38*Helper(999999),Fvar(16)

[State 0, BindToRoot] ; LOSE
type = BindToRoot
trigger1 = root,stateno = 170 && root,time > 10
pos = -9*Helper(999999),Fvar(16),-27*Helper(999999),Fvar(16)

[State 0, Trans]
type = Trans
trigger1 = 1
trans = add

[State 0, AssertSpecial]
type = AssertSpecial
trigger1 = 1
flag = Noshadow

[State 0, AssertSpecial]
type = AssertSpecial
triggerall = root,stateno != [120,155]
trigger1 = root,movetype = H || root,stateno = 5120
trigger2 = root,stateno = 2310 && root,time = [6,15]
trigger3 = root,stateno = 30200 && root,time = [33,42]
trigger4 = root,stateno = 190 || root,stateno = 2190 && root,animelemtime(4) < 0
trigger5 = root,stateno = 30101 && root,time < 11
flag = Invisible

[State 200, End]
type = Destroyself
trigger1 = root,var(2) = 0
